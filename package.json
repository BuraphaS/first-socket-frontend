{
  "name": "vue3-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite",
    "build": "bunx --bun vite build",
    "build:alpha": "bunx --bun vite build --mode development",
    "build:staging": "bunx --bun vite build --mode staging",
    "preview": "bunx --bun vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "test": "vitest --watch --reporter verbose",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@primeuix/themes": "^1.2.5",
    "@primevue/forms": "^4.5.1",
    "axios": "^1.13.2",
    "humps": "^2.0.1",
    "js-cookie": "^3.0.5",
    "pinia": "^2.3.1",
    "pinia-plugin-persistedstate": "^4.7.1",
    "primeicons": "^7.0.0",
    "primevue": "^4.5.1",
    "socket.io-client": "^4.8.3",
    "tailwindcss-primeui": "^0.6.1",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@pinia/testing": "^1.0.3",
    "@primevue/auto-import-resolver": "^4.5.1",
    "@stylistic/eslint-plugin": "^3.1.0",
    "@types/humps": "^2.0.6",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.19.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^9.33.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.5.2",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "unplugin-vue-components": "^28.8.0",
    "vite": "^6.4.1",
    "vite-plugin-eslint2": "^5.0.4",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix"
    ]
  }
}